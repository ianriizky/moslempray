{
    "name": "ianriizky/moslempray",
    "description": "API Manager to find pray time for Moslem developed for PHP and Laravel Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ianriizky",
            "email": "ian.rizkypratama@gmail.com"
        }
    ],
    "keywords": [
        "moslem pray",
        "waktu sholat",
        "laravel",
        "php"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": ">=5.0",
        "illuminate/http": ">=7.0",
        "illuminate/support": ">=7.0",
        "spatie/data-transfer-object": "^1.0",
        "spatie/enum": "^2.0"
    },
    "require-dev": {
        "filp/whoops": ">=1.0",
        "phpunit/phpunit": ">7.0",
        "symfony/var-dumper": ">=2.0",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Ianrizky\\MoslemPray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ianrizky\\MoslemPray\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ianrizky\\MoslemPray\\ServiceProvider"
            ],
            "aliases": {
                "MoslemPray": "Ianrizky\\MoslemPray\\Facades\\MoslemPray"
            }
        }
    }
}
