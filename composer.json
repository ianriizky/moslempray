{
    "name": "ianriizky/moslempray",
    "description": "API Manager to find pray time for Moslem developed for PHP and Laravel Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ianriizky",
            "email": "ian.rizkypratama@gmail.com"
        }
    ],
    "keywords": [
        "moslem pray",
        "waktu sholat",
        "laravel",
        "php"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/http": "^8.36",
        "illuminate/support": "^8.36",
        "spatie/data-transfer-object": "^1.13",
        "spatie/enum": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "^2.12",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Ianrizky\\MoslemPray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ianrizky\\MoslemPray\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ianrizky\\MoslemPray\\ServiceProvider"
            ],
            "aliases": {
                "MoslemPray": "Ianrizky\\MoslemPray\\Facades\\MoslemPray"
            }
        }
    }
}
